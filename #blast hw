#blast hw

$ conda activate blast

cd ~/Desktop

makeblastdb -in '/home/peliah/Desktop/database_mystery_transcripts.fasta' -dbtype nucl -out mystery_nucl
 
 blastn -query '/home/peliah/Desktop/query_mystery_nucleotide.fasta' -db mystery_nucl -evalue 1e-3 -max_target_seqs 5 -outfmt 7 -out transcripts_results.txt

 #What is/are the IDs/sequence names of most similar hit(s) from the above search (based on sequence identity)?
 #IDS: XM_072069044.1, XM_072069045.1, XM_072069045.1

awk '$3 > 90 {print $2}' <transcripts_results.txt> > most_similar.txt

makeblastdb -in '/home/peliah/Desktop/database_mystery_proteins.fasta' -dbtype prot -out mystery_prot

blastp -query '/home/peliah/Desktop/query_mystery_proteins.fasta' -db mystery_prot -evalue 1e-5 -max_target_seqs 15 -outfmt 7 -out proteins_results.txt

What is/are the IDs/sequence names of the best hit(s) from the above search (based on e-value)?
#XP_027148892.1, KAL2505989.1, WKA09249.1, WKA09249.1, CDP22058.1, CDP22058.1

